{
  "name": "clean-components",
  "version": "1.0.8",
  "description": "Clean, modular, tested, react components for everyday needs.",
  "type": "module",
  "main": "./dist/index.mjs",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bn-l/clean-components.git"
  },
  "scripts": {
    "start:dev": "ts-node",
    "lint:style": "npx eslint --fix",
    "lint:react": "npx eslint --config ./eslintrc.react.json --ext .ts,.tsx,.mts source",
    "storybook-dev": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest --run --coverage --exclude \"**/*online*\"",
    "test:single": "vitest --bail=1",
    "test:online": "vitest online --run --bail=1",
    "test:all": "vitest --run --coverage",
    "lint": "npx eslint --ext .mts",
    "build": "tsc --build --incremental",
    "coverageBadge": "tsx scripts/coverage-badge.ts",
    "build:clean": "rimraf ./dist && tsc --build",
    "publish-ci": "tsx scripts/publish-ci.ts",
    "vite-dev": "vite",
    "buildReadme": "tsx scripts/readme-helper.mts",
    "vite-build": "tsc && vite build",
    "vite-preview": "vite preview",
    "pub": "npx bumpp"
  },
  "keywords": [
    "clean",
    "components",
    "react",
    "darkmode",
    "switch",
    "tested"
  ],
  "license": "MIT",
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.2",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/addon-interactions": "^8.0.8",
    "@storybook/addon-links": "^8.0.8",
    "@storybook/blocks": "^8.0.8",
    "@storybook/react": "^8.0.8",
    "@storybook/react-vite": "^8.0.8",
    "@storybook/test": "^8.0.8",
    "@stylistic/eslint-plugin": "^1.7.0",
    "@swc/core": "^1.3.101",
    "@types/core-js": "^2.5.8",
    "@types/node": "^20.12.5",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vanilla-extract/css": "^1.14.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^1.4.0",
    "badgen": "^3.2.3",
    "c8": "^9.1.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-sort-class-members": "^1.20.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "eslint-plugin-unicorn": "^52.0.0",
    "glob": "^10.3.12",
    "rimraf": "^5.0.5",
    "sinon": "^17.0.1",
    "storybook": "^8.0.8",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.1.0",
    "unocss": "^0.59.0",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.8.3",
    "vite-plugin-lib-inject-css": "^2.0.1",
    "vitest": "^1.4.0",
    "zx": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "colord": "^2.9.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "colord": "^2.9.3"
  },
  "dependencies": {}
}
